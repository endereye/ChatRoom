<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="170.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="chatroom.implement.client.ui.UIInit">
    <Button layoutX="199.0" layoutY="250.0" mnemonicParsing="false" onAction="#signIn" text="登录"
            AnchorPane.bottomAnchor="20.0" AnchorPane.leftAnchor="100.0" AnchorPane.rightAnchor="100.0"/>
    <TextField fx:id="username" layoutX="145.0" layoutY="62.0" promptText="用户名" AnchorPane.leftAnchor="50.0"
               AnchorPane.rightAnchor="50.0" AnchorPane.topAnchor="20.0"/>
    <PasswordField fx:id="password" layoutX="145.0" layoutY="76.0" promptText="密码" AnchorPane.leftAnchor="50.0"
                   AnchorPane.rightAnchor="50.0" AnchorPane.topAnchor="70.0"/>
</AnchorPane>
